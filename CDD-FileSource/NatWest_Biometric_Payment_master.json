{
	"objects": [
		{
			"application": "Local|NatWest-Online-Payments",
			"name": "master", 
			"kind": "ApplicationVersion"
		},
		{
			"application": "Local|NatWest-Money-Transfer",
			"name": "master",
			"kind": "ApplicationVersion"
		},
		{
			"application": "Local|NatWest-Credit-Score",
			"name": "master",
			"kind": "ApplicationVersion"
		},
		{
			"runOnCreation": false,
			"applicationVersions": [
				"Local|NatWest-Money-Transfer/master",
				"Local|NatWest-Credit-Score/master",
				"Local|NatWest-Online-Payments/master"
			],
			"isReplaceApplicationVersionOnChangeNotification": false,
			"markAsDonePhaseName": "",
			"version": "master",
			"name": "NatWest Biometric Payment",
			"kind": "Release",
			"ownerParties": [
				"superuser@ca.com"
			]
		},
		{
			"release": "NatWest Biometric Payment/master",
			"isProduction": false,
			"scope": "RELEASE_SCOPE",
			"value": "",
			"name": "CHANGE_REQUEST_CONTENT",
			"kind": "ReleaseToken"
		},
		{
			"release": "NatWest Biometric Payment/master",
			"isProduction": false,
			"scope": "RELEASE_SCOPE",
			"value": "",
			"name": "CHANGE_REQUEST_DESCRIPTION",
			"kind": "ReleaseToken"
		},
		{
			"release": "NatWest Biometric Payment/master",
			"isProduction": false,
			"scope": "RELEASE_SCOPE",
			"value": "",
			"name": "CHANGE_REQUEST_ID",
			"kind": "ReleaseToken"
		},
		{
			"release": "NatWest Biometric Payment/master",
			"environments": [
				"DEV"
			],
			"approvalGate": "AUTOMATIC",
			"skipTasksWithUnchangedApplications": false,
			"isApprovalRequired": false,
			"name": "DEV",
			"kind": "Phase"
		},
		{
			"release": "NatWest Biometric Payment/master",
			"environments": [
				"QA"
			],
			"approvalGate": "MANUAL",
			"skipTasksWithUnchangedApplications": false,
			"isApprovalRequired": false,
			"previousPhase": "DEV",
			"name": "QA",
			"kind": "Phase"
		},
		{
			"release": "NatWest Biometric Payment/master",
			"environments": [
				"UAT"
			],
			"approvalGate": "MANUAL",
			"skipTasksWithUnchangedApplications": false,
			"isApprovalRequired": false,
			"previousPhase": "QA",
			"name": "UAT",
			"kind": "Phase"
		},
		{
			"release": "NatWest Biometric Payment/master",
			"environments": [
				"PRODUCTION"
			],
			"approvalGate": "MANUAL",
			"skipTasksWithUnchangedApplications": false,
			"isApprovalRequired": false,
			"previousPhase": "UAT",
			"name": "PRODUCTION",
			"kind": "Phase"
		},
		{
			"phase": "NatWest Biometric Payment/master/DEV",
			"isDisabled": false,
			"applicationVersionBuilds": [
				{
					"buildNumber": "%NatWest-Money-Transfer.last_successful_change%",
					"applicationVersion": "Local|NatWest-Money-Transfer/master"
				},
				{
					"buildNumber": "%NatWest-Credit-Score.last_successful_change%",
					"applicationVersion": "Local|NatWest-Credit-Score/master"
				}
			],
			"isOnFailurePhaseEnabled": true,
			"isSkipTaskOnFailure": false,
			"name": "Deploy NatWest Biometric Payment",
			"kind": "Task",
			"plugin": "Ansible Core/1.4",
			"endpoint": "AnsibleCore",
			"parameters": {
				"inventoryPath": "inventories/inventory.txt",
				"parallelProcessCount": "5",
				"playbookPath": "playbooks/first_local_playbook.yml",
				"branch": "master",
				"verbosity": "Normal"
			},
			"type": "Run Ansible Playbook"
		},
		{
			"phase": "NatWest Biometric Payment/master/DEV",
			"isDisabled": false,
			"applicationVersionBuilds": [
				{
					"buildNumber": "%NatWest-Online-Payments.last_successful_change%",
					"applicationVersion": "Local|NatWest-Online-Payments/master"
				}
			],
			"isOnFailurePhaseEnabled": true,
			"isSkipTaskOnFailure": false,
			"name": "Deploy NatWest Online Payments",
			"kind": "Task",
			"plugin": "Tomcat/1.0",
			"endpoint": "Tomcat",
			"parameters": {
				"tomcatContextPath": "/dummy",
				"packageGroup": "dummy",
				"packageVersion": "1.0",
				"packageArtifact": "dummy",
				"buildNumber": "%NatWest-Online-Payments.last_successful_change%"
			},
			"type": "Deploy Artifact"
		},
		{
			"phase": "NatWest Biometric Payment/master/QA",
			"isDisabled": false,
			"applicationVersionBuilds": [
				{
					"buildNumber": "%NatWest-Credit-Score.last_successful_change%",
					"applicationVersion": "Local|NatWest-Credit-Score/master"
				}
			],
			"isOnFailurePhaseEnabled": true,
			"isSkipTaskOnFailure": false,
			"name": "Create Change Request",
			"kind": "Task",
			"plugin": "ServiceNow/2.2",
			"endpoint": "ServiceNow",
			"parameters": {
				"changeRequestId": "%CHANGE_REQUEST_ID%",
				"changeRequestContent": "%CHANGE_REQUEST_CONTENT%",
				"shortDescription": "%CHANGE_REQUEST_DESCRIPTION%"
			},
			"type": "Create Change Request"
		},
		{
			"phase": "NatWest Biometric Payment/master/UAT",
			"isDisabled": false,
			"isOnFailurePhaseEnabled": true,
			"isSkipTaskOnFailure": false,
			"name": "Wait for Approval",
			"kind": "Task",
			"plugin": "ServiceNow/2.2",
			"endpoint": "ServiceNow",
			"parameters": {
				"changeRequestId": "%CHANGE_REQUEST_ID%",
				"pollInterval": "2",
				"statusField": "status",
				"requiredStatus": "Approved",
				"ticketType": "Change Request"
			},
			"type": "Wait For Approval"
		}
	]
}
